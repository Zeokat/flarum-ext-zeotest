{
	"name": "zeokat/flarum-ext-folinks",
	"description": "Follow domain links.",
	"type": "flarum-extension",
	"keywords": ["flarum","links","follow","nofollow","seo"],
	"author": {
		"name": "Zeokat"
	},
	"license": "MIT",
	"require": {
		"php": ">=5.4.0",
		"flarum/core": "^0.1.0-beta.3"
	},
	"autoload": {
            "psr-4": {
                "Zeokat\\FollowAutolink\\": "src/"
            }
        },
	"support": {
		"source": "https://github.com/Zeokat/flarum-ext-folinks",
